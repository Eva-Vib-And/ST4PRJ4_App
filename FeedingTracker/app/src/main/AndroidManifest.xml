        android:supportsRtl="true"
        android:theme="@style/Theme.FeedingTracker.Girly">
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartFeedingByBottle"
            android:exported="false" />
        <activity
            android:name=".ChooseFeedingType"
            android:exported="true" />
        <activity
            android:name=".StartFeedingByBreast"
            android:exported="false" />
        <activity
            android:name=".MilkPlanActivity"
            android:exported="false" />
        <activity
            android:name=".OverviewAcitivity"
            android:exported="false" />
        <activity
            android:name=".ChooseMethodActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>