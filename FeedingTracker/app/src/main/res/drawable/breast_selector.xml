<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:drawable="@drawable/bra">
        <shape android:shape="rectangle">
            <corners android:radius="8dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:state_checked="true" android:drawable="@drawable/bra">
        <shape android:shape="rectangle">
            <corners android:radius="8dp"/>
            <solid android:color="@color/pink_cf2"/>
        </shape>
    </item>
</selector>